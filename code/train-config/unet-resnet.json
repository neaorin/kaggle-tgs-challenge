{
    "version": "1",
    "name": "unet-resnet-%s",
    "image_augmentation": {
        "flip_horizontal": true,
        "flip_vertical": false
    },
    "encoder": {
        "predefined": false,
        "type": "unet-resnet",
        "weights": "none"
    },
    "training": {
        "stage1": {
            "epochs": 100,
            "batch_size": 16,
            "optimizer": {
                "name": "adam",
                "initial_learning_rate": 0.01
            },
            "loss": "binary_crossentropy",
            "keep_weights_frozen_epochs": 0
        },
        "stage2": {
            "epochs": 80,
            "batch_size": 16,
            "optimizer": {
                "name": "adam",
                "initial_learning_rate": 0.005
            },
            "loss": "lovasz" 
        }
    }
}