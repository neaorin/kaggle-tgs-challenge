{
    "version": "3",
    "name": "unet-pretrained-encoder-resnet34-%s",
    "crossvalidation": {
        "folds": 4
    },
    "input": {
        "size": 224,
        "channels": 3,
        "normalize": false
    },
    "encoder": {
        "type": "Unet",
        "backbone": "resnet34",
        "weights": "imagenet"
    },
    "stage1": {
        "epochs": 80,
        "epochs_weights_frozen": 0,
        "batch_size": 32
    },
    "stage2": {
        "epochs": 80,
        "batch_size": 32
    }
}